<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="bijian.model.bean">
    <class name="Mood" dynamic-insert="true"  dynamic-update="true">
       <id name="moodID">
          <generator class="identity"/>
       </id>
       <property name="content" column="content" type="string" not-null="true"/>
       <property name="createTime" column="createTime" type="date" not-null="true"/>
       
       <many-to-one name="author" column="author" class="User" not-null="true" cascade="none"/>
       <many-to-one unique="true" name="origin" column="origin" class="User" not-null="false" cascade="none"/>
       
    
    </class>

</hibernate-mapping>
