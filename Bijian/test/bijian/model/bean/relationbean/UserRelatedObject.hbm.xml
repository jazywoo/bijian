<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="bijian.model.bean.relationbean">
    <class name="UserRelatedObject" table="userRelatedObject" dynamic-update="true"  dynamic-insert="true">
       <id name="userRelatedObjectID" column="userRelatedObjectID" type="long">
          <generator class="native"/>
       </id>
       <property name="isObjectActive" column="isObjectActive" type="java.lang.Integer" ></property>
       <property name="objectType" column="objectType" type="java.lang.Integer" ></property>
       <property name="createTime" column="createTime" type="java.util.Date"/>
       
       <!--  多对一    -->
       <many-to-one name="user" column="userID" class="bijian.model.bean.User" cascade="none"/>
       <many-to-one name="object" column="objectID" class="bijian.model.bean.ArticleObject" cascade="none"/>
       
    </class>


</hibernate-mapping>